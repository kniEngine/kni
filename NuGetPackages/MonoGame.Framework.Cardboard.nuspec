        <dependency id="nkast.Xna.Framework.Graphics" version="$version$" />
        <dependency id="nkast.Xna.Framework.Audio" version="$version$" />
        <dependency id="nkast.Xna.Framework.Media" version="$version$" />
    </dependencies>
    
  </metadata>
